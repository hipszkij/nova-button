{
    "name": "hipszkij/nova-button",
    "description": "(Nova 4+) A Laravel Nova package for adding buttons to your resources.",
    "authors": [
        {
            "name": "Janos Hipszki",
            "email": "hipszkij@gmail.com"
        },
        {
            "name": "Danique Wijnalda",
            "email": "hello@dnwjn.dev"
        },
        {
            "name": "Brian Dillingham",
            "email": "bdillingham88@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "button",
        "nova4",
        "field"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "hipszkij\\NovaButton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "hipszkij\\NovaButton\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
